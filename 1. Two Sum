class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        
     std::unordered_map<int, int> value;
        std::vector<int> vect;
        for(int i =0 ; i < nums.size(); i++){
            int difference = target - nums[i];
            if(value.find(nums[i]) != value.end()){
                vect.push_back(value[nums[i]]);
                vect.push_back(i);
                return vect;
            }
            
            value.insert(make_pair(difference,i));
            //cout<< value[difference] << endl;

        }
        return vect;
        
    }
};
