class MyQueue {
public:
    stack<int> myObj;

    MyQueue() {
        
        
    }
    
    void push(int x) {
        
        stack<int> temp;
        while(!myObj.empty()) {
            temp.push(myObj.top());
            myObj.pop();
        }
        temp.push(x);
        while(!temp.empty()){
            myObj.push(temp.top());
            temp.pop();
        }
        
    }
    
    int pop() {
        
        int i = myObj.top();
        myObj.pop();
        return i;
        
        
    }
    
    int peek() {
        return myObj.top();
        
    }
    
    bool empty() {
        if(myObj.empty())
            return true;
        else
            return false;
    }
};

/**
 * Your MyQueue object will be instantiated and called as such:
 * MyQueue* obj = new MyQueue();
 * obj->push(x);
 * int param_2 = obj->pop();
 * int param_3 = obj->peek();
 * bool param_4 = obj->empty();
 */
