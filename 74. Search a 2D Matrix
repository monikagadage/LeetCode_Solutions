class Solution {
public:
    bool searchMatrix(vector<vector<int>>& matrix, int target) {
        
        
        for(int i =0; i < matrix.size(); i++){
            int last = matrix[i].size() -1;
            bool svalue= false;
            if( target <= matrix[i][last] ){
                svalue=true;
                int low = 0;
                int high = last;
                while(low<=high){
                    int mid = low+ (high-low)/2;
                    cout << matrix[i][mid] << endl;

                    if(matrix[i][mid]< target)
                        low = mid+1;
                    else if(matrix[i][mid] > target)
                        high = mid-1;
                    else{
                        return true;
                    }
                        
                }
            }
            if(svalue == true){
                return false;
            }
        }
        return false;
        
    }
};
